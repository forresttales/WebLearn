<h1>institutes new</h1>

<div class="wrapper row3">
  <div id="container" class="clear">
    <div id="content">
      <div id="comments" style="height: 480px">

			<div>
				<%= link_to "Return", institutes_path %>
			</div>
			<h2></h2>      	
	        <h2>School Create Login</h2>
			<br />
		    <div id="content">
		      <div id="respond">
				
				<%= form_for(:institutes, :url => {:action => 'create'}) do |f| %>
		      	
					<table summary="User Institutes Create Password">
						
					  <tr>
					    <th><%= f.label(:username) %></th>
					    <td><%= f.text_field(:username) %></td>
					  </tr>
					  <tr>
					    <th><%= f.label(:password) %></th>
					    <td><%= f.password_field(:password) %></td>
					  </tr>
					  <tr>
					  	<th><%= f.label :password_confirmation, "Confirmation" %></th>
					  	<td><%= f.password_field :password_confirmation %></td>
					  </tr>					  
					  <tr>
					  	<th>
					  		
					  	</th>
					  	<td>
				      		<%= submit_tag('Submit') %>
					  	</td>
					  </tr>
					  
					</table>
			  
			  	<% end %>
		        
		      </div>
		      
		    </div>
        
      </div>
    </div>
  </div>
</div>
