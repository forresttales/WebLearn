				j = 0;
				k = 0;
				l = 0;
				m = 0;
				// n = 0;
				p = 0;
				s = 0;
				t = 0;
				// jj = 0;
				kk = 0;	
				ll = 0;	
				mm = 0;
				dj = 0;
				djj = 0;
