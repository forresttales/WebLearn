		function scrollToAnchor(aid) {
			var a = $("a[name='" + aid + "']");
			$('html, body').animate({ scrollTop: a.offset().top }, 1000);
			// jQuery('html, body').animate({scrollTop: 100}, 1000);				
		}
            
		function in_archive_ids(ar, id) {
			b = false;
		    for(var i = 0; i < ar.length; i++) {
		        if(ar[i] != null) {
		        	b = true;
		        }
		    }
		    return b;
		}
		

		$(window).load( function() {
			
			
			$(".article-type-1").each(function() {
				var id = $(this).attr("id");
				if(id != null) {
					if(in_archive_ids(gon.archives, id)) {
						var archive_img_path 	= gon.archives[id][1];
						var archive_type 		= gon.archives[id][2];
						var archive_slug 		= gon.archives[id][3];
						var archive_subtitle_1 	= gon.archives[id][4];
						var archive_subtitle_2 	= gon.archives[id][5];
						var archive_author 		= gon.archives[id][6];
						// var archive_img_height	= gon.archives[id][7];
		
						// thoughts
				   		// research
				   		// idea
				   		// strategy
				   		// innovation
						var s_tmpl = null;
						s_tmpl = "templates/article_template_carousel_" + archive_type.toLowerCase();
						$.tmpl(s_tmpl, {
							src_image: archive_img_path,
							slug: archive_slug,
							subtitle_1: archive_subtitle_1,
							subtitle_2: archive_subtitle_2,
							author: archive_author 							
						}).appendTo("#" + id.toString());
					}
					else {
						alert("page id = " + id.toString() + " not found in database");
					}
				}
			});					
						
			$(".article-type-2").each(function() {
				var id = $(this).attr("id");
				if(id != null) {
					if(in_archive_ids(gon.archives, id)) {
						var archive_img_path 	= gon.archives[id][1];
						var archive_type 		= gon.archives[id][2];
						var archive_slug 		= gon.archives[id][3];
						var archive_subtitle_1 	= gon.archives[id][4];
						var archive_subtitle_2 	= gon.archives[id][5];
						var archive_author 		= gon.archives[id][6];
						var archive_img_height	= gon.archives[id][7];
		
						// design
				   		// spotlight
				   		// innovation
				   		// ideas
				   		// event news
						var s_tmpl = null;
						var s_tmpl_sub = null;
						if(archive_subtitle_2.length > 0) {
							s_tmpl_sub = "1b";
						}
						else {
							s_tmpl_sub = "1a";
						}
						s_tmpl = "templates/article_template_" + archive_type.toLowerCase() + "_" + s_tmpl_sub;
						$.tmpl(s_tmpl, {
							img_width: 300,
							img_height: archive_img_height,
							type: "design",
							slug: archive_slug,
							src_image: archive_img_path,
							subtitle_1: archive_subtitle_1,
							subtitle_2: archive_subtitle_2,
							author: archive_author 
						}).appendTo("#" + id.toString());
					}
					else {
						alert("page id = " + id.toString() + " not found in database");
					}
				}				
			});					
						
			$(".article-type-3").each(function() {
