						// console.log('jj = ' + jj);
						// j = j >= jj ? j : jj;
						// var n = parseInt(j);
						var n = parseInt(jj);	
						// console.log('n = ' + n);
						n += 36;
						var upperCell = $(this).find('.tmpl-div-2a');
						upperCell.height(n);
					}
					if ($(this).hasClass('tmpl-outercell-double')) {
						if (!(djj = $(this).find("img").attr("height"))) {
							djj = 0;
						}
						// console.log('djj = ' + djj);
						dj = dj >= djj ? dj : djj;
						dn = parseInt(dj);																		
					}
				});
					// n += 6;
					// var upperCell = $(this).find('.tmpl-div-2a');
					// upperCell.height(n);
				// });

				$(this).children().each( function() {
					if ($(this).hasClass('tmpl-outercell')) {
						kk = $(this).height();
						if (!(kk = $(this).height())) {
							kk = 0;
						}
						k = k >= kk ? k : kk;
						t = k;
